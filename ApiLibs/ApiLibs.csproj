<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>10.0</LangVersion>
    <PackageReleaseNotes>To see what is new, go to https://github.com/mjwsteenbergen/ApiLibs</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/mjwsteenbergen/ApiLibs</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mjwsteenbergen/ApiLibs.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>An easily extendable restclient library for csharp </Description>
    <Authors>Martijn Steenbergen</Authors>
    <Version>2.0.0</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="RestSharp" Version="109.0.1" />
    <PackageReference Include="HttpMultipartParser" Version="5.1.0" />
    <PackageReference Include="OneOf" Version="3.0.243" />
    <PackageReference Include="Martijn.Extensions" Version="1.15.0" /> 
  </ItemGroup>
</Project>
