<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <PackageReleaseNotes>To see what is new, go to https://github.com/mjwsteenbergen/ApiLibs</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/mjwsteenbergen/ApiLibs</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mjwsteenbergen/ApiLibs.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Description>An easily extendable restclient library for csharp </Description>
    <Authors>Martijn Steenbergen</Authors>
    <Version>1.1.0</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.JSON" Version="12.0.3" /> 
    <PackageReference Include="RestSharp" Version="106.6.7" />
    <PackageReference Include="Microsoft.Bcl.AsyncInterfaces" Version="1.1.0" />
    <PackageReference Include="Martijn.Extensions" Version="1.8.0" />
  </ItemGroup>
</Project>
